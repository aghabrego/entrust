{
  "name": "weirdo/entrust",
  "description": "Entrust es una forma sucinta y flexible de agregar permisos basados en roles",
  "license": "MIT",
  "authors": [
    {
      "name": "Angel Hidalgo",
      "email": "aghabrego@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://gitlab.com/grupoprodecasa/helper.git"
    }
  ],
  "require": {
    "php": "^8.0|^8.1",
    "illuminate/console": "8.*|9.*",
    "illuminate/support": "8.*|9.*",
    "illuminate/config": "8.*|9.*",
    "illuminate/cache": "8.*|9.*",
    "illuminate/database": "8.*|9.*",
    "illuminate/container": "8.*|9.*",
    "laravel/helpers": "^1.3",
    "weirdo/helper": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3@dev",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Weirdo\\Entrust\\": "src/Entrust/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Weirdo\\Entrust\\EntrustServiceProvider"
      ],
      "aliases": {
        "Entrust": "Weirdo\\Entrust\\EntrustFacade"
      }
    }
  }
}