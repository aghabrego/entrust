{
  "name": "weirdo/entrust",
  "description": "Entrust es una forma sucinta y flexible de agregar permisos basados en roles",
  "license": "MIT",
  "authors": [
    {
      "name": "Angel Hidalgo",
      "email": "aghabrego@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://gitlab.com/grupoprodecasa/helper.git"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/console": "~5.7|5.8|6.*|7.*|8.*",
    "illuminate/support": "~5.7|5.8|6.*|7.*|8.*",
    "illuminate/config": "~5.7|5.8|6.*|7.*|8.*",
    "illuminate/cache": "~5.7|5.8|6.*|7.*|8.*",
    "illuminate/database": "~5.7|5.8|6.*|7.*|8.*",
    "illuminate/container": "~5.7|5.8|6.*|7.*|8.*",
    "laravel/helpers": "^1.3",
    "weirdo/helper": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3@dev",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Weirdo\\Entrust\\": "src/Entrust/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra":{
    "laravel":{
      "providers":[
        "Weirdo\\Entrust\\EntrustServiceProvider"
      ],
      "aliases":{
        "Entrust": "Weirdo\\Entrust\\EntrustFacade"
      }
    }
  }
}
